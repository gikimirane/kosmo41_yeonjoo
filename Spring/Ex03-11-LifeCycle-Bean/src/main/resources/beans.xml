<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
						http://www.springframework.org/schema/context 
						http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	
	<context:annotation-config />
	
	<bean id="student" class="com.study.spring.Student">
		<constructor-arg value="로다주"></constructor-arg>
		<constructor-arg value="53"></constructor-arg>		
 	</bean>
 	
 	<bean id="otherstudent" class="com.study.spring.OtherStudent">
		<constructor-arg value="베네딕트컴버배치"></constructor-arg>
		<constructor-arg value="42"></constructor-arg>		
 	</bean>

</beans>

